r = function (num, fixed) {\n    if (num === null) { return null; } // terminate early\n    if (num === 0) { return '0'; } // terminate early\n    fixed = (!fixed || fixed < 0) ? 0 : fixed; // number of decimal places to show\n    var b = (num).toPrecision(4).split(\"e\"), // get power\n        k = b.length === 1 ? 0 : Math.floor(Math.min(b[1].slice(1), 14) / 3), // floor at decimals, ceiling at trillions\n        c = k < 1 ? num.toFixed(0 + fixed) : (num / Math.pow(10, k * 3)).toFixed(1 + fixed), // divide by power\n        d = c < 0 ? c : Math.abs(c), // enforce -0 is 0\n        e = d + ['', 'k', 'm', 'b', 't'][k]; // append power\n    return e;\n}\n\nfunction getCol(arr) {\n    var col = \"\"\n    if (parseFloat(arr[0]) > arr[arr.length - 1]) {\n        col = \"#ea3943\"\n    }\n    else if (parseFloat(arr[0]) < arr[arr.length - 1]) {\n        col = \"#16c784\"\n    }\n    else {\n        col = \"#16c784\"\n    }\n    return (col)\n}","import Navbar from './components/Navbar';\nimport Sidebar from './components/Sidebar';\nimport { BrowserRouter as Router, Route, Switch } from 'react-router-dom';\nimport Tokens from './Tokens';\nimport Tokenpage from './components/Tokenpage';\n\nfunction App() {\n  return (\n    <Router>\n      <div className=\"App\">\n        <Navbar />\n        <Sidebar />\n        <div className=\"content\">\n          <Switch>\n            <Route exact path=\"/\">\n              <p>Kardia info - Super early edition</p>\n            </Route>\n            <Route exact path=\"/tokens\">\n              <Tokens />\n            </Route>\n            <Route exact path=\"/lps\">\n              This page will display the value of lps\n            </Route>\n            <Route path=\"/tokens/:id\" component={Tokenpage}/>\n          </Switch>\n        </div>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\n\nReactDOM.render(\n  <React.StrictMode>\n    